<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="u">

	<insert id="user_insert" parameterType="m_user">
		insert into m_user values(
								0,
								#{id},
								#{pwd},
								#{birth},
								#{email},
								#{tel}
								)
	</insert>
	
</mapper>












